<script>
function availableFormatter(value, row) {
    var icon = row.available === true ? 'fa-check' : 'fa-times';
    return '<i class="fa ' + icon + '"></i> ';
}

function availableSorter(a, b) {
	return (a === b)? 0 : a? -1 : 1;
